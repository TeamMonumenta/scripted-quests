{
    "title": "Static Title",
    "type": "object",
    "description": "A static title. Equivalent to a /title command. At least one of title or subtitle should be supplied.",
    "options": {
        "keep_oneof_values": false
    },
    "properties": {
        "type": {
            "type": "string",
            "default": "static",
            "options": {
                "hidden": true
            }
        },
        "stay": {
            "propertyOrder": 1,
            "required": true,
            "title": "Stay",
            "description": "How long the title will stay (in ticks)",
            "type": "integer",
            "minimum": 1
        },
        "fadeIn": {
            "propertyOrder": 2,
            "required": false,
            "title": "Fade In",
            "description": "Fade in (in ticks)",
            "type": "integer"
        },
        "fadeOut": {
            "propertyOrder": 3,
            "required": false,
            "title": "Fade Out",
            "description": "Fade out (in ticks)",
            "type": "integer"
        },
        "title": {
            "propertyOrder": 4,
            "title": "Title",
            "description": "The Title (can be left blank).",
            "type": "string",
            "default": ""
        },
        "subtitle": {
            "propertyOrder": 5,
            "title": "Subtitle",
            "description": "The Subtitle (can be left blank).",
            "type": "string",
            "default": ""
        }
    }
}
