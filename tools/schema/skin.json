{
	"title": "Player Skin Texture",
	"defaultProperties": [
			"name",
			"texture",
			"signature"
	],
	"additionalProperties": false,
	"properties": {
		"name": {
			"required": true,
			"title": "name",
			"propertyOrder": 1,
			"description": "A name for the skin.",
			"type": "string",
			"pattern": "[-+._A-Za-z0-9]+"
		},
		"texture": {
			"required": true,
			"title": "texture",
			"propertyOrder": 2,
			"description": "texture link to mojang servers base64 encoded",
			"type": "string",
			"pattern": "^[-A-Za-z0-9+/]+={0,3}$",
			"default": ""
		},
		"signature": {
			"required": false,
			"title": "texture",
			"propertyOrder": 3,
			"description": "signature for above texture encoded",
			"type": "string"
		}
	}
}